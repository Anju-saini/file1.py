try:
    n1= int(input("enter any numbr"))
    n2=int(input("enter any numbr"))
    print("divesion",n1/n2)
except ZeroDivisionError:
    print("DON'T entrer zero")
except  ValueError:
    print("plez enter a specif value")
finally:
    print("program end")



